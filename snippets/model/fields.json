{
    "Field.blank": {
        "prefix": ["blank","field"],
        "body": [
            "blank=${1|True,False|"
        ],
        "description": "If True, the field is allowed to be blank. Default is False."
    },
    "Field.null": {
        "prefix": ["null","field"],
        "body": [
            "null=${1|True,False|"
        ],
        "description": "If True, Django will store empty values as NULL in the database. Default is False."
    },
    "Choices - Dict": {
        "prefix": ["choices-dict","choices","field"],
        "body": [
            "$1_CHOICES = {",
            "\t${2:key}: ${3:value},",
            "}"
        ],
        "description": "A mapping or iterable in the format described below to use as choices for this field."
    },
    "Choices - Sequence": {
        "prefix": ["choices-sequence","choices","field"],
        "body": [
            "$1_CHOICES = [",
            "\t(${2:key}, ${3:value}),",
            "]"
        ],
        "description": "A mapping or iterable in the format described below to use as choices for this field."
    },
    "Field.choices": {
        "prefix": ["choices-attribute","choices","field"],
        "body": ["choices=${1}_CHOICES$0"],
        "description": "db.models.choices"
    },
    "Choices - Option Group": {
        "prefix": ["choices-group","choices","field"],
        "body": [
            "$1_CHOICES = [",
            "\t(",
            "\t\t\"$2\", (",
            "\t\t\t(\"${3:key}\", \"${4:value}\"),",
            "\t\t)",
            "\t)",
            "\t(\"unknown\", \"unknown\")",
            "]"
        ],
        "description": ""
    },
